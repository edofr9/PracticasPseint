Algoritmo Ejercicio1Cooperativo
	
	Definir n, i, j Como Entero
	
	para i <- 1 hasta 5 Hacer
		
		Escribir "Ingrese numero ", i
		
		leer n
		
		si n >= 1 Y n <= 20 Entonces
			
			Escribir n Sin Saltar
			
			para j <- 1 hasta n Hacer
				
				Escribir "*" sin saltar
				
			FinPara
			
		SiNo
			
			Escribir "El numero no esta en el rango"
			
			i = i - 1 
			
		FinSi
		
		Escribir ""
		
				
		
	FinPara
	
	
	
FinAlgoritmo
