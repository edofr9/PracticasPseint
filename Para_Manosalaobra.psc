Algoritmo Para_Manosalaobra
	
	Definir i , num, numMax Como Entero
	
	numMax = 0
	
	para i <- 1 Hasta 5 Con Paso 1
		
		Escribir "Ingrese un numero entero "
		leer num
		
		si num > numMax Entonces
			
			numMax=num
			
		FinSi
		
		
	FinPara
	
	Escribir "El numero mayor ingresado fue " numMax
	
FinAlgoritmo
